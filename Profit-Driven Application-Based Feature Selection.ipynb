    "#The free base auc below is the AUC from using free features only\n",
    "free_base_auc = 0.539 \n",
    "\n",
    "#This is done for operational reasons to separate the free feature line from the data frame\n",
    "xgb_df[\"feature_subset\"] = xgb_df[\"Feature Group\"]\n",
    "xgb_app_group = xgb_df[ xgb_df['feature_subset'].str.len() == 1]\n",
    "xgb_df = xgb_df[ xgb_df['feature_subset'].str.len() > 1]\n",
    "\n",
    "\n",
    "#creating table with AUC gain per cost\n",
    "xgb_df[\"Individual Feature Cost\"] = xgb_df[\"Feature Group\"].apply(get_indiv_feature_cost , costs_dictionary=cost_dict, row_num = len(validate))\n",
    "\n",
    "#Calculated by the difference of the current AUC minus the free base AUC\n",
    "xgb_df[\"AUC gain\"] = xgb_df[\"AUC\"] - free_base_auc\n",
    "#Finally, the result above is divided to the current cost\n",
