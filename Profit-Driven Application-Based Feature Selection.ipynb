   "source": [
    "#bayes optimal threshold, that means our predictions need to be higher or equal than this to be rounded to a 1\n",
    "threshold = np.mean( cost_matrix['false_positive'] - cost_matrix['true_negative']  ) / np.mean( cost_matrix['false_negative'] + cost_matrix['false_positive'] - cost_matrix['true_negative'] - cost_matrix['true_positive'] )\n",
    "threshold"
   ]
