   "source": [
    "train = train.rename(index = str, columns = {\"BAD\": \"GOOD\"})\n",
    "train[\"GOOD\"] = train.GOOD.eq('GOOD').mul(1)\n",
    "\n",
    "validate = validate.rename(index = str, columns = {\"BAD\": \"GOOD\"})\n",
    "validate[\"GOOD\"] = validate.GOOD.eq('GOOD').mul(1)\n",
    "\n",
    "test = test.rename(index = str, columns = {\"BAD\": \"GOOD\"})\n",
    "test[\"GOOD\"] = test.GOOD.eq('GOOD').mul(1)\n",
    "\n",
    "#Split the lables from train, validate and test data\n",
    "\n",
    "y_train = train[\"GOOD\"]\n",
    "y_validate = validate[\"GOOD\"]\n",
    "y_test = test[\"GOOD\"]"
   ]
